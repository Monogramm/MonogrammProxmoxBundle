{
    "name": "monogramm/proxmox-bundle",
    "type": "symfony-bundle",
    "description": "ProxmoxVE API integration in Symfony",
    "keywords": ["proxmox", "api", "symfony", "bundle"],
    "homepage": "http://www.monogramm.io",
    "license": "AGPL-3.0-or-later",
    "authors": [
        {
            "name": "Monogramm",
            "homepage": "https://github.com/Monogramm"
        }
    ],
    "require": {
        "php": ">=5.5",
        "symfony/dependency-injection" : ">=2.3 <4.1.4",
        "symfony/config" : ">=2.3 <4.1.4",
        "symfony/http-kernel": ">=2.3 <4.1.4",
        "zzantares/proxmoxve": "4.0.0"
    },
    "autoload": {
        "psr-4": { "Monogramm\\ProxmoxBundle\\": "" }
    },
    "require-dev": {
        "phpspec/phpspec": "~2.1",
        "leanphp/phpspec-code-coverage": "~2.1",
        "squizlabs/php_codesniffer": "~3.0"
    }
}
